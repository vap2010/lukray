<h1>Listing admin_users</h1>

<table border="0" cellspacing="0" cellpadding="4" class="tdborders_ccc">
  <thead>
    <tr>
      <th>Blocked</th>
      <th>Type</th>
      <th>Status</th>
      <th>Role</th>
      <th>Phone</th>
      <th>Mobile</th>
      <th>Name</th>
      <th>Surname</th>
      <th>Patronymic</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_users.each do |admin_user| %>
      <tr>
        <td><%= admin_user.blocked %></td>
        <td><%= admin_user.type_id %></td>
        <td><%= admin_user.status_id %></td>
        <td><%= admin_user.role_id %></td>
        <td><%= admin_user.phone %></td>
        <td><%= admin_user.mobile %></td>
        <td><%= admin_user.name %></td>
        <td><%= admin_user.surname %></td>
        <td><%= admin_user.patronymic %></td>
        <td><%= link_to 'Show', admin_user %></td>
        <td><%= link_to 'Edit', edit_admin_user_path(admin_user) %></td>
        <td><%= link_to 'Destroy', admin_user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User', new_admin_user_path %>
